{
  "name": "@rljson/cli",
  "version": "0.0.7",
  "packageManager": "pnpm@10.6.2",
  "description": "The RLJSON command line interface",
  "homepage": "https://github.com/rljson/cli",
  "bugs": "https://github.com/rljson/cli/issues",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=22.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rljson/cli.git"
  },
  "main": "dist/main.js",
  "bin": {
    "rljson": "./dist/cli.cjs"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "node build.js && tsc && cp README* dist",
    "test": "npx vitest run --coverage && npm run lint",
    "prebuild": "npm run test",
    "prepublishOnly": "npm run build && npm run test",
    "lint": "npx eslint",
    "updateGoldens": "cross-env UPDATE_GOLDENS=true npm test"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.0.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "eslint-plugin-jsdoc": "^59.1.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^16.4.0",
    "jsdoc": "^4.0.4",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.6",
    "vite-node": "^3.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-dom": "^0.1.1"
  },
  "dependencies": {
    "@rljson/converter": "^0.0.22",
    "@rljson/hash": "^0.0.16",
    "@rljson/json": "^0.0.21",
    "@rljson/rljson": "^0.0.56",
    "@rljson/validate": "^0.0.11",
    "chalk": "^5.6.2",
    "commander": "^14.0.1"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
